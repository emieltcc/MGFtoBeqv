Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["D", "TI"], "5"], "\[LongEqual]", 
         UnderoverscriptBox["\[Sum]", 
          RowBox[{
            SubscriptBox[
             StyleBox["p", "TI"], "1"], "\[Ellipsis]", 
            SubscriptBox[
             StyleBox["p", "TI"], "5"]}], "\[Prime]", LimitsPositioning -> 
          True], 
         FractionBox[
          RowBox[{"\[Delta]", "(", 
            RowBox[{
              SubscriptBox[
               StyleBox["p", "TI"], "1"], "+", 
              SubscriptBox[
               StyleBox["p", "TI"], "2"], "+", 
              SubscriptBox[
               StyleBox["p", "TI"], "3"], "+", 
              SubscriptBox[
               StyleBox["p", "TI"], "4"], "+", 
              SubscriptBox[
               StyleBox["p", "TI"], "5"]}], ")"}], 
          RowBox[{"|", 
            SubscriptBox[
             StyleBox["p", "TI"], "1"], 
            SuperscriptBox["|", "2"], "|", 
            SubscriptBox[
             StyleBox["p", "TI"], "2"], 
            SuperscriptBox["|", "2"], "|", 
            SubscriptBox[
             StyleBox["p", "TI"], "3"], 
            SuperscriptBox["|", "2"], "|", 
            SubscriptBox[
             StyleBox["p", "TI"], "4"], 
            SuperscriptBox["|", "2"], "|", 
            SubscriptBox[
             StyleBox["p", "TI"], "5"], 
            SuperscriptBox["|", "2"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "D_5=\\sum_{p_1\\dots \
p_5}'\\frac{\\delta(p_1+p_2+p_3+p_4+p_5)}{|p_1|^2|p_2|^2|p_3|^2|p_4|^2|p_5|^2}\
", "state" -> "Boxes"|>,
